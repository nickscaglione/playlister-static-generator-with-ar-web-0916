<h1>Genre Page</h1>
<h2>Name: <%= @genre.name %></h2>
<ul><% @genre.artists.each do |artist| %>
        <li><h4><a href="/artists/<%=artist.to_slug%>.html"><%= artist.name %></a></h4></li>
    <% end %>
</ul>
<ul><% @genre.songs.each do |song| %>
        <li><h4><a href="/songs/<%=song.to_slug%>.html>"><%= song.artist.name %> - <%= song.name %> - <%= song.genre.name %></a></h4></li>
    <% end %>
</ul>
